{
    "require": {
        "php": "7.4.27",
        "predis/predis": "^1.1",
        "roave/security-advisories": "dev-master"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Framework\\" : "psr/Framework",
            "Framework\\Application\\" : "psr/Framework/Application",
            "Tests\\" : "tests",
            "App\\" : "./"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "scripts": {
        "test" : "phpunit tests/Framework/Http/RequestTest.php --colors=always"
    }
}
